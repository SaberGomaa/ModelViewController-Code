@using MVCDayOne.Models;

@{

    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Dispaly</title>
</head>
<body>
    <div>

        <table>

            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>address</th>
                <th>age</th>
                <th>Dept</th>
            </tr>
            @{
                foreach (var item in (List<Student>)ViewBag.st) { 
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.Name</td>
                            <td>@item.Address</td>
                            <td>@item.Age</td>
                            <td>@item.Department.Name</td>
                        </tr>
                }
            }
        </table>

    </div>
</body>
</html>
